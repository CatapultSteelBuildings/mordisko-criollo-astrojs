---
import Layout from '@layouts/Layout.astro';
import ProductsMainSection from '@components/products/products-main-section.astro';

interface Props {
  location: string;
}

const { location } = Astro.props;

const title = 'Productos';
const metaDescription = 'Productos';
---

<Layout location={location} title={title} metaDescription={metaDescription}>
  <ProductsMainSection />
</Layout>
